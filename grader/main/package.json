{
    "name": "igt",
    "productName": "igt",
    "version": "2018.8.11",
    "license": "custom",
    "author": "Deb Banerji",
    "description": "Intelligent Grading Tool",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clean": "rm -rf dist && rm -rf ../release-builds && rm -rf ../installers",
        "build-electron": "ng build --base-href . && cp src/electron/* -r dist && (cd src/electron && npm install)",
        "build-linux-32": "npm run build-electron && (cd dist && electron-packager . --overwrite --platform=linux --arch=ia32 --icon=assets/images/logo_blue.png --prune=true --out=../../release-builds)",
        "build-linux-64": "npm run build-electron && (cd dist && electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/images/logo_blue.png --prune=true --out=../../release-builds)",
        "build-windows-32": "npm run build-electron && (cd dist && electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/images/logo_blue.png --prune=true --out=../../release-builds)",
        "build-windows-64": "npm run build-electron && (cd dist && electron-packager . --overwrite --platform=win32 --arch=x64 --icon=assets/images/logo_blue.png --prune=true --out=../../release-builds)",
        "build-mac-64": "npm run build-electron && (cd dist && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/images/logo_blue.png --prune=true --out=../../release-builds)",
        "installer-deb-32": "npm run build-linux-32 && electron-installer-debian --src ../release-builds/igt-linux-ia32/ --dest ../installers/ --arch i386 --icon=src/assets/images/100_emoji.png",
        "installer-deb-64": "npm run build-linux-64 && electron-installer-debian --src ../release-builds/igt-linux-x64/ --dest ../installers/ --arch amd64 --icon=src/assets/images/100_emoji.png",
        "installer-windows-32": "npm run build-windows-32 && node create-windows-installer-32.js && mv ../installers/windows-32/igtSetup.exe ../installers/igt-setup-32.exe && rm -rf ../installers/windows-32",
        "installer-windows-64": "npm run build-windows-64 && node create-windows-installer-64.js && mv ../installers/windows-64/igtSetup.exe ../installers/igt-setup-64.exe && rm -rf ../installers/windows-64",
        "electron": "npm run build-electron && electron dist",
        "start": "npm run electron"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.0.7",
        "@angular/cdk": "6.3.1",
        "@angular/common": "^6.0.7",
        "@angular/compiler": "^6.0.7",
        "@angular/core": "^6.0.7",
        "@angular/forms": "^6.0.7",
        "@angular/http": "^6.0.7",
        "@angular/material": "6.3.1",
        "@angular/platform-browser": "^6.0.7",
        "@angular/platform-browser-dynamic": "^6.0.7",
        "@angular/router": "^6.0.7",
        "core-js": "^2.4.1",
        "decompress": "^4.2.0",
        "dictionary-en-us": "^2.0.0",
        "hammerjs": "^2.0.8",
        "nodeify": "^1.0.1",
        "nspell": "^2.0.1",
        "rxjs": "^6.2.1",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.8",
        "@angular/cli": "6.0.8",
        "@angular/compiler-cli": "^6.0.7",
        "@angular/language-service": "^6.0.7",
        "@types/jasmine": "2.8.8",
        "@types/node": "^10.5.1",
        "codelyzer": "~4.4.2",
        "electron": "1.6.11",
        "electron-packager": "8.7.2",
        "electron-winstaller": "^2.6.3",
        "jasmine-core": "~3.1.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.4",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^2.0.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^1.1.0",
        "protractor": "~5.3.2",
        "ts-node": "~7.0.0",
        "tslint": "~5.10.0",
        "typescript": "~2.7.2"
    },
    "optionalDependencies": {
        "electron-installer-dmg": "^1.0.0",
        "electron-installer-debian": "^0.8.1"
    }
}
